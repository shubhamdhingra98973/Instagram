ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AnimatedCollectionViewLayout" "$PODS_CONFIGURATION_BUILD_DIR/Fusuma" "$PODS_CONFIGURATION_BUILD_DIR/JSSAlertView" "$PODS_CONFIGURATION_BUILD_DIR/Sharaku" "$PODS_CONFIGURATION_BUILD_DIR/SwiftyCam"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/AnimatedCollectionViewLayout/AnimatedCollectionViewLayout.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Fusuma/Fusuma.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/JSSAlertView/JSSAlertView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Sharaku/Sharaku.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftyCam/SwiftyCam.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AnimatedCollectionViewLayout" -framework "Fusuma" -framework "JSSAlertView" -framework "Sharaku" -framework "SwiftyCam"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
